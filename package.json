{
  "name": "lince-player",
  "config": {
    "npmTitle": "LINCE PLUS video player"
  },
  "version": "0.0.0",
  "description": "Video player used in LINCE PLUS",
  "author": "Alberto Soto Fernandez",
  "keywords": [
    "react",
    "video",
    "video js",
    "youtube",
    "player",
    "LINCE PLUS"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/lince-player.umd.js",
  "module": "./dist/lince-player.es.js",
  "exports": {
    ".": {
      "import": "./dist/lince-player.es.js",
      "require": "./dist/lince-player.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "typescript": "^5.2.2",
    "video.js": "^8.5.2",
    "videojs-youtube": "^3.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^1.0.7",
    "c8": "^7.11.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^19.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "vite": "^2.8.0",
    "vitest": "^0.3.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
